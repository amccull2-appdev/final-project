<h1>
  All ETFs
</h1>

<table class="table">
  <tr>

    <th>
      Ticker
    </th>

    <th>
      Name
    </th>

    <th>
      Category
    </th>

    <th>
    </th>
  </tr>

  <% @etfs.each do |etf| %>
    <tr>

      <td>
      <a href="/etfs/<%= etf.id %>">
        <%= etf.ticker %>
      </a>
      </td>

      <td>
        <%= etf.name %>
      </td>

      <td>
        <%= etf.category %>
      </td>

    </td>
  </tr>
<% end %>
</table>
